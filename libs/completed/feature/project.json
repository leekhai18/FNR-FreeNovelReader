{
  "name": "completed-feature",
  "projectType": "library",
  "sourceRoot": "libs/completed/feature/src",
  "prefix": "fnr",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/completed/feature"],
      "options": {
        "jestConfig": "libs/completed/feature/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/completed/feature/src/**/*.ts",
          "libs/completed/feature/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["type:feature", "scope:fnr"]
}
