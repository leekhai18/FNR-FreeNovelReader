{
  "name": "shell-ui-layout",
  "projectType": "library",
  "sourceRoot": "libs/shell/ui/layout/src",
  "prefix": "fnr",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shell/ui/layout"],
      "options": {
        "jestConfig": "libs/shell/ui/layout/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shell/ui/layout/src/**/*.ts",
          "libs/shell/ui/layout/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["type:ui", "scope:fnr"]
}
